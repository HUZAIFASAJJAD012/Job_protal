{
    "version": 2,
    "name": "hiring_lawyer",
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.js",
            "methods": ["GET", "POST", "PUT", "DELETE"]
        }
    ]
}
